{
  "name": "demo-coda",
  "type": "module",
  "version": "1.0.0",
  "description": "Round robin load balancer",
  "main": "gateway.ts",
  "scripts": {
    "gateway": "npm run build && node dist/gateway.js",
    "worker": "npm run build && node dist/worker.js",
    "worker1": "npm run build && PORT=3001 node dist/worker.js",
    "worker2": "npm run build && PORT=3002 node dist/worker.js",
    "worker3": "npm run build && PORT=3003 node dist/worker.js",
    "all-workers": "npm run build && concurrently \"PORT=3001 node dist/worker.js\"  \"PORT=3002 node dist/worker.js\" \"PORT=3003 node dist/worker.js\"",
    "start": "npm run build && concurrently \"node dist/gateway.js\" \"PORT=3001 node dist/worker.js\"  \"PORT=3002 node dist/worker.js\" \"PORT=3003 node dist/worker.js\"",
    "test": "npm run build && c8 tap run",
    "lint": "eslint .",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "keywords": [],
  "author": "Phuong Nguyen",
  "license": "To be updated",
  "dependencies": {
    "@fastify/sensible": "^5.0.0",
    "@fastify/under-pressure": "^8.3.0",
    "@fastify/url-data": "^5.3.1",
    "@types/async-lock": "^1.4.1",
    "async-lock": "^1.4.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.0.0",
    "fastify-plugin": "^4.0.0",
    "fastify-raw-body": "^4.2.2",
    "random": "^4.1.0",
    "toad-scheduler": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/proxyquire": "^1.3.30",
    "@types/sinon": "^10.0.20",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "fastify-tsconfig": "^1.0.1",
    "prettier": "3.0.3",
    "sinon": "^17.0.0",
    "tap": "^18.5.2",
    "typescript": "^5.2.2"
  }
}
